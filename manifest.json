{
    "manifest_version": 2,
    "name": "Better Facebook",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*"
            ],
            "js": [
                "better-facebook.js"
            ],
            "css": [
                "better-facebook.css"
            ],
            "run_at": "document_end"
        }
    ]
}
